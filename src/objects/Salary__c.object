    <validationRules>
        <fullName>Required_to_fill_Bank_Details</fullName>
        <active>true</active>
        <description>Bank account number should not be null</description>
        <errorConditionFormula>LEN( Bank_Details__c  )   =  0</errorConditionFormula>
        <errorDisplayField>Bank_Account_Number__c</errorDisplayField>
        <errorMessage>Enter the deatils of bank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restricting_salaryInactive_employee</fullName>
        <active>true</active>
        <errorConditionFormula>AND( Credited_to__r.Is_active__c = FALSE,
         Credited_to__r.Is_on_notice_period__c =FALSE)</errorConditionFormula>
        <errorDisplayField>Basic__c</errorDisplayField>
        <errorMessage>Salary cannot be processed for inactive employees.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_of_Bank_Account_Number</fullName>
        <active>true</active>
        <description>Account number of bank should not be changed once entered</description>
        <errorConditionFormula>ISCHANGED(Bank_Account_Number__c)</errorConditionFormula>
        <errorDisplayField>Bank_Account_Number__c</errorDisplayField>
        <errorMessage>Bank account number can not be changed.</errorMessage>
    </validationRules>
