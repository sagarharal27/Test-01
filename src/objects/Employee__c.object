    <validationRules>
        <fullName>Age_validation</fullName>
        <active>true</active>
        <description>validates age between 21-30</description>
        <errorConditionFormula>Age__c   &lt; 21 || Age__c  &gt;31</errorConditionFormula>
        <errorMessage>Age should be in between 21-30</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Notice_period</fullName>
        <active>true</active>
        <description>Checking notice period</description>
        <errorConditionFormula>IF( Is_on_notice_period__c  = TRUE,
       ISNULL( Resignation_date__c ),
       NOT (ISNULL(Resignation_date__c)  )
    )</errorConditionFormula>
        <errorMessage>Please check the resignation date and notice period</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SSN_limit_upto_10</fullName>
        <active>true</active>
        <description>Limit size upto 10 to SSN</description>
        <errorConditionFormula>SSN__c &lt; 10</errorConditionFormula>
        <errorDisplayField>SSN__c</errorDisplayField>
        <errorMessage>SSN should be less than 10</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Valid_Date_of_resignation</fullName>
        <active>true</active>
        <description>Check validation of resignation</description>
        <errorConditionFormula>Resignation_date__c &lt;  Date_of_joining__c</errorConditionFormula>
        <errorMessage>Enter date greater than Joining date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_of_DOB</fullName>
        <active>true</active>
        <errorConditionFormula>TODAY()  -  Date_of_Birth__c  &lt; 18</errorConditionFormula>
        <errorDisplayField>Date_of_Birth__c</errorDisplayField>
        <errorMessage>Please provide a valid Date of birth. Employee cannot be less than 18 years of Age.</errorMessage>
    </validationRules>
