    <script>
       j$( document ).ready(function() {
            j$('.desktopView').show();
            j$('.mobileHeader').hide();
             j$('.showPopup').hide();
      });
      j$(window).resize(function(){
            mobileSidebarFix();
      });
      function mobileSidebarFix() {
         if( j$('body').width() <= 979 ) {
               j$('.desktopView').hide();
               j$('.mobileHeader').show();
               j$('.hidePanel').show();
               j$('.showPopup').hide();
               j$('body').removeClass( "bodyBackground" );
          }
          else {
                j$('body').removeClass( "bodyBackground" );
                j$('.desktopView').show();
                j$('.mobileHeader').hide();
                j$('.showPopup').hide();
             }
      }
      function callTabsMethod() {
        j$('.showMenuNavigation').show();
      }
      function callSearchMethod() {
         console.log('HIee..');
         j$('.showPopup').show();
         j$('.hidePanel').hide();
         j$('body').addClass( "bodyBackground" );
      }
      function callCancelMethod() {
          j$('body').removeClass( "bodyBackground" );
          j$('.showPopup').hide();
          j$('.hidePanel').show();
      }
      
                   /*** Component Related ***/
      function handle(e) {
         if(e.keyCode === 13) {
             flag = 1;
             e.preventDefault();
             //setTimeout(function() {
                 searchRecords(); 
             //}, delay);
             return false;
         } else {
            flag = 0;
         }
     }
     function searchRecords() {
        if(document.getElementById('searchBox')){
            var searchName = '';
            searchName = document.getElementById('searchBox').value;
            objName = j$('#objectName').val();
            j$(".ui-menu").hide();
            var urlString = '/apex/aeSearch?searchText='+searchName+'&sObjectName='+objName;
            window.open(urlString);
            j$('#objectName').val('');
        }
     }

     
